<form>

  <div class="container col-md-8 order-md-1">
    <%= form_tag url_for(action: 'create'), method: "post" do %>
      <%= label_tag 'Date :' %>
        <%= text_field_tag 'start_date', "",class: "form-control"  %>
      <%= label_tag 'Titre :' %>
        <%= text_field_tag 'title',"", class: "form-control"  %>
      <%= label_tag 'description :' %>
        <%= text_field_tag 'description', "",class: "form-control"  %>
      <%= label_tag 'Ville :' %>
        <%= text_field_tag 'location', "",class: "form-control"  %>
      <%= label_tag 'Prix :' %>
        <%= text_field_tag 'price', "",class: "form-control"  %>
    <%= submit_tag "Confirmer", class: "btn btn-lg btn-block btn-primary mt-3" %>
    <% end %>


  </form>
